syntax on
set hlsearch
set tabstop=4
set backspace=indent,eol,start
filetype plugin indent on
set scrolloff=3
set showcmd
set t_Co=256
set confirm
set statusline=%f%m%r%h%w\ [FORM,ENC=%{&ff},%{&encoding}\]\ [TYPE=%Y]\ [ASCII=\%03.3b]\ [HEX=\%02.2B]\ [POS=%04l,%04v][%p%%]\ [LEN=%L]
set laststatus=2
set wrap
set autoindent
colorscheme desert

